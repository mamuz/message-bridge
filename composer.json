{
  "name": "mamuz/message-bridge",
  "type": "library",
  "description": "Message bridge to support a decoupled event driven and aspect oriented application",
  "homepage": "https://github.com/mamuz/message-bridge",
  "license": "MIT",
  "keywords": [
    "message",
    "event",
    "aop",
    "bridge"
  ],
  "authors": [
    {
      "name": "Marco Muths",
      "email": "mamuz.de@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/mamuz/message-bridge/issues",
    "source": "https://github.com/mamuz/message-bridge"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5"
  },
  "autoload": {
    "files": [
      "src/message_functions.php"
    ],
    "psr-4": {
      "MsgBridge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MsgBridgeTest\\": "tests/"
    }
  }
}
